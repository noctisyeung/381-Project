<html>
<head>
    <title><%=title.vaild%></title>
<script>function goBack(){
    
    window.history.back();   
}</script>
</head>
<body>
    <% if (title.vaild == "Error"){%>
        <h1>Error</h1>
        <p>You are not the owner!!</p>
        <button onclick="goBack()">Back</button>
    <%}else{%>
    <h1>Modify Restaurant test</h1>
    <form action="/change" method="post">
    <p>
    <input type="hidden" name="_id" value="<%=restaurant._id%>">
  
    <input required name="name" required value="<%=restaurant.name%>" >
    <label>Name</label></p>
    <p>
    <input name="borough"class="" type="text" value="<%=restaurant.borough%>" >
    <label class="">Borough</label></p>
    <p>
    <input name="cuisine"class="" type="text" value="<%=restaurant.cuisine%>" >
    <label class="">Cuisine</label></p>
    <p>
    <input name="street"class="" type="text" value="<%=restaurant.address.street%>" >
    <label class="">Street</label></p>
    <p>
    <input name="building"class="" type="text" value="<%=restaurant.address.building%>" >
    <label class="">Building</label></p>
    <p>
    <input name="zipcode"class="" type="text" value="<%=restaurant.address.zipcode%>" >
    <label class="">Zipcode</label></p>
    <p>
    <p>
    <input name="lon"class="" type="text" value="<%=restaurant.address.coord[0]%>" >
    <label class="">GPS Coordinates (lon.)</label></p>
    <p>
    <input name="lat"class="" type="text" value="<%=restaurant.address.coord[1]%>" >
    <label class="">GPS Coordinates (lat.)</label></p>
    <p>
    <input name="sampleFile"class="" type="file" >
    <label class="">Photo</label></p>
    <p>
        <button type = 'submit'>Edit</button></p>
  </form>





<%}%>

</body>
</html>